pipeline {
    agent any
    
    stages {
        stage('Checkout') {
            steps {
                // Checkout the code from the Git repository
                git 'https://github.com/syammarolix/vprofile.git'
            }
        }
        
        stage('Build') {
            steps {
                 def mavenHome = tool name:"maven-3.9.3", type:"maven"
                 def mavenCMD = "${mavenHome}/bin/mvn"
                 sh "${mavenCMD} test"
        
             }
       }
   }
}
