node{
stage('Get Code'){ 
git credentialsId: '3eae5f90-92f6-4acf-9c4d-a62e5c93d7e9', url: 'https://github.com/syammarolix/vprofile.git'
 }

  stage('Build package'){
        def mavenHome = tool name:"maven-3.9.3", type:"maven"
        def mavenCMD = "${mavenHome}/bin/mvn"
        sh "${mavenCMD} clean package"
     }

stage('sonarqube'){
            def mavenHome = tool name:"maven-3.9.3", type:"maven"
            def mavenCMD = "${mavenHome}/bin/mvn"
            sh "${mavenCMD} sonar:sonar"
     }
}
