node{
stage('Get Code'){ 
git credentialsId: '6fce09a8-32d9-4e1a-91f4-79f798c06547', url: 'https://github.com/syammarolix/vprofile.git'
 }

  stage('Build package'){
        def mavenHome = tool name:"maven-3.9.3", type:"maven"
        def mavenCMD = "${mavenHome}/bin/mvn"
        sh "${mavenCMD} clean package"
     }

stage('sonarqube'){
            def mavenHome = tool name:"maven-3.9.3", type:"maven"
            def mavenCMD = "${mavenHome}/bin/mvn"
            sh "${mavenCMD} sonar:sonar"
     }
stage('nexus'){
          nexusArtifactUploader artifacts: [[artifactId: 'vproapp', classifier: '', file: 'target/vprofile-v2.war', type: 'war']], credentialsId: '7a79b735-8078-422a-b0f2-72564e22849a', groupId: 'v.in', nexusUrl: '3.88.173.149:8081', nexusVersion: 'nexus3', protocol: 'http', repository: 'vprofile-release', version: '2.0'
       }
}
