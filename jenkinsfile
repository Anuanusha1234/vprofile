node{
stage('Get Code'){ 
git branch: '*/master', credentialsId: 'e8505514-8949-46fa-8fc2-536d211a37b4', url: 'https://github.com/syammarolix/vprofile.git'
 }

  stage('Build package'){
        def mavenHome = tool name:"maven-3.9.3", type:"maven"
        def mavenCMD = "${mavenHome}/bin/mvn"
        sh "${mavenCMD} clean package"
  }
}
